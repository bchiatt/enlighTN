#home
  h1 Upcoming Events
.row
  .col-xs-1
  .col-xs-10.current
    .events(ng-repeat='event in events')
      h4 {{event.name}}
      p {{event.type}}
  .col-xs-1
.row.trending
  .col-xs-5.twitter
    h2.text-center Twitter
  .col-xs-2
  .col-xs-5.rss-feed.pull-right
    h2.text-center RSS Feed


