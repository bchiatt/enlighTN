#home
  h1 Upcoming Events
  .row
    .col-xs-1
    .col-xs-10.current
      .col-xs-1
      .events.col-xs-10
        h4(ng-repeat='event in events') {{event.name}}
      .col-xs-1
    .col-xs-1
  .row.trending
    .col-xs-1
    .col-xs-4.twitter
      h2.text-center Twitter
    .col-xs-2
    .col-xs-4.rss-feed
      h2.text-center RSS Feed
      kj-parse(feeds='feeds')
    .col-xs-1


