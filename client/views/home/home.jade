#home
  h1 Upcoming Events
  .row
    .col-xs-1
    .col-xs-10.current
      .col-xs-1
      .events.col-xs-10
        h4(ng-repeat='event in events') {{event.name}}
      .col-xs-1
    .col-xs-1
  .row.trending
    .col-xs-1
      h2.twitter-header Twitter
    .col-xs-4.twitter
    .col-xs-2
      h2.rss-header RSS&nbspFeed
    .col-xs-4.rss-feed
      kj-parse(feeds='feeds')
    .col-xs-1


